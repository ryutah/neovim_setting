{
  "languageserver": {
    "kotlin": {
      "command": "~/.local/nvim/server/bin/kotlin-language-server",
      "filetypes": [
        "kotlin"
      ]
    }
  },
  "snippets.extends": {
    "javascriptreact": [
      "javascript"
    ],
    "typescriptreact": [
      "javascript"
    ],
    "typescript": [
      "javascript"
    ]
  },
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips",
    "snippets/ultisnips"
  ],
  "python.jediEnabled": false,
  "go.goplsOptions": {
    "usePlaceholders": true
  }
}
